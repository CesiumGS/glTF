{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "mesh.primitive.EXT_implicit_geometry.schema.json",
    "title": "EXT_primitive_voxels glTF Mesh Primitive extension",
    "type": "object",
    "description": "`EXT_implicit_geometry` extension for a primitive in a glTF model, to define implicit 3D geometry",
    "allOf": [
        {
            "$ref": "glTFProperty.schema.json"
        }
    ],
    "properties": {
        "box": {
            "$ref": "box.schema.json"
        },
        "cylinder": {
            "$ref": "cylinder.schema.json"
        },
        "sphere": {
            "$ref": "sphere.schema.json"
        },
        "ellipsoid": {
            "$ref": "ellipsoid.schema.json"
        },
        "region": {
            "$ref": "region.schema.json"
        },
    },
    "oneOf": [
        {
            "required": "box"
        },
        {
            "required": "cylinder"
        },
        {
            "required": "sphere"
        },
        {
            "required": "ellipsoid"
        },
        {
            "required": "region"
        }
    ]
}